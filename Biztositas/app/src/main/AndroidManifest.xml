<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.biztositas">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Biztositas">
        <receiver
            android:name=".AlarmReceiver"
            android:enabled="true"
            android:exported="false"></receiver>

        <activity
            android:name=".CarModifyActivity"
            android:exported="false" />
        <activity
            android:name=".AddInsuranceActivity"
            android:exported="false"
            android:label="@string/title_activity_add_insurance"
            android:parentActivityName=".CarsActivity"
            android:theme="@style/Theme.Biztositas.NoActionBar" />
        <activity
            android:name=".NewCarActivity"
            android:exported="false" />
        <activity
            android:name=".CarsActivity"
            android:exported="false" />
        <activity
            android:name=".RegisterActivity"
            android:exported="false" />
        <activity
            android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>